{
  "name": "keylessh-stun-server",
  "version": "1.0.0",
  "type": "module",
  "description": "STUN/TURN relay server with signaling for NAT traversal and P2P connectivity",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild src/bundle-tidecloak.js --bundle --format=iife --platform=browser --outfile=public/tidecloak.js --minify && tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts"
  },
  "dependencies": {
    "@tidecloak/js": "^0.13.16",
    "jose": "^6.1.3",
    "keycloak-js": "^26.2.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.0",
    "@types/ws": "^8.5.13",
    "esbuild": "^0.27.3",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3"
  }
}
